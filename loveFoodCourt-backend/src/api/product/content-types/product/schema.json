{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true,
      "maxLength": 72,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 3
    },
    "price": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "imageURL": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "KFC",
        "McDonalds"
      ]
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "discountPrice": {
      "type": "float",
      "required": false
    }
  }
}
